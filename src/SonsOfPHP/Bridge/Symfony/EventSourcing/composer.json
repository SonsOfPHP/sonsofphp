{
    "name": "sonsofphp/event-sourcing-symfony",
    "type": "library",
    "description": "Adds additional functionality to the Sons of PHP Event Sourcing library",
    "keywords": [
        "event-sourcing"
    ],
    "homepage": "https://github.com/SonsOfPHP/event-sourcing-symfony",
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Estes",
            "email": "joshua@sonsofphp.com"
        }
    ],
    "support": {
        "issues": "https://github.com/SonsOfPHP/sonsofphp/issues",
        "forum": "https://github.com/orgs/SonsOfPHP/discussions",
        "docs": "https://docs.sonsofphp.com"
    },
    "autoload": {
        "psr-4": {
            "SonsOfPHP\\Bridge\\Symfony\\EventSourcing\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "sonsofphp/event-sourcing": "^0.3.x-dev",
        "symfony/uid": "^5 || ^6 || ^7",
        "symfony/security-bundle": "^6 || ^7"
    },
    "require-dev": {
        "symfony/http-foundation": "^5 || ^6 || ^7",
        "symfony/messenger": "^5 || ^6 || ^7",
        "symfony/serializer": "^5 || ^6 || ^7"
    },
    "extra": {
        "sort-packages": true,
        "branch-alias": {
            "dev-main": "0.3.x-dev"
        }
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/JoshuaEstes"
        },
        {
            "type": "tidelift",
            "url": "https://tidelift.com/subscription/pkg/packagist-sonsofphp-sonsofphp"
        }
    ]
}