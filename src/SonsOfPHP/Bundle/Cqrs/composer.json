{
    "name": "sonsofphp/cqrs-bundle",
    "type": "symfony-bundle",
    "description": "CQRS Symfony Bundle",
    "minimum-stability": "dev",
    "keywords": [
        "cqrs",
        "symfony",
        "command",
        "query",
        "event sourcing"
    ],
    "homepage": "https://github.com/SonsOfPHP/cqrs-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Estes",
            "email": "joshua@sonsofphp.com"
        }
    ],
    "support": {
        "issues": "https://github.com/SonsOfPHP/sonsofphp/issues",
        "forum": "https://github.com/orgs/SonsOfPHP/discussions",
        "docs": "https://docs.sonsofphp.com"
    },
    "autoload": {
        "psr-4": {
            "SonsOfPHP\\Bundle\\Cqrs\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SonsOfPHP\\Bundle\\Cqrs\\Tests\\": "Tests/"
        }
    },
    "require": {
        "php": ">=8.2",
        "sonsofphp/cqrs": "^0.3.x-dev",
        "symfony/dependency-injection": "^5 || ^6 || ^7",
        "symfony/http-kernel": "^5 || ^6 || ^7",
        "symfony/messenger": "^5 || ^6 || ^7"
    },
    "suggest": {
        "sonsofphp/event-sourcing": "Get some of that sweet ass event sourcing action"
    },
    "extra": {
        "sort-packages": true,
        "branch-alias": {
            "dev-main": "0.3.x-dev"
        }
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/JoshuaEstes"
        },
        {
            "type": "tidelift",
            "url": "https://tidelift.com/subscription/pkg/packagist-sonsofphp-sonsofphp"
        }
    ]
}