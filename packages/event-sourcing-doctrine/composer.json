{
    "name": "sonsofphp/event-sourcing-doctrine",
    "type": "library",
    "description": "Adds additional functionality to the Sons of PHP Event Sourcing library",
    "keywords": [
        "event-sourcing"
    ],
    "homepage": "https://github.com/SonsOfPHP/event-sourcing",
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Estes",
            "email": "joshua@sonsofphp.com"
        }
    ],
    "support": {
        "issues": "https://github.com/SonsOfPHP/sonsofphp/issues",
        "forum": "https://github.com/orgs/SonsOfPHP/discussions",
        "docs": "https://docs.sonsofphp.com",
        "chat": "https://discord.gg/sdVxNhFqND"
    },
    "autoload": {
        "psr-4": {
            "SonsOfPHP\\Bridge\\Doctrine\\EventSourcing\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "doctrine/dbal": "^3",
        "sonsofphp/event-sourcing": "^0.3@dev"
    },
    "extra": {
        "sort-packages": true,
        "branch-alias": {
            "dev-main": "0.3.x-dev"
        }
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/JoshuaEstes"
        },
        {
            "type": "tidelift",
            "url": "https://tidelift.com/subscription/pkg/packagist-sonsofphp-sonsofphp"
        }
    ]
}
